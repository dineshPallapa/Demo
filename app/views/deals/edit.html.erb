<div class="col-md-6 col-md-offset-3">
  <div class="signup-form">
    <%= simple_form_for @deal, html: { multipart: true } do |f| %>
      <%= f.input :category_id, collection: @categories, prompt: "Select a catagory" %>
      <%= f.input :image, as: :file %>
      <%= f.input :title %>
      <%= f.input :description %>
      <%= f.input :link %>
      <%= f.input :link1 %>
      <%= f.button :submit %> 
    <% end %>
  </div>
</div>